{
  "name": "stencil-angular",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "author": "",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./dist/components": {
      "types": "./dist/types/components/index.d.ts",
      "import": "./dist/components/index.js"
    }
  },
  "scripts": {
    "build": "npm run tsc && npm run copy-files",
    "copy-files": "cp -r src/components/stencil-generated dist/components/",
    "clean": "rm -rf node_modules .turbo dist",
    "tsc": "tsc -p . --outDir ./dist"
  },
  "types": "dist/types/index.d.ts",
  "dependencies": {
    "@angular/core": "^18.2.0",
    "rxjs": "~7.8.0",
    "stencil-library": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "typescript": "^5.5.2"
  },
  "publishConfig": {
    "access": "public"
  }
}