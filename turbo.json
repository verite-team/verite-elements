{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "install": {
      "dependsOn": [],
      "outputs": []
    },
    "build": {
      "dependsOn": [
        "^install"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "app-install": {
      "dependsOn": [
        "packages-build"
      ],
      "outputs": []
    },
    "packages-build": {
      "dependsOn": [
        "^install",
        "^packages-build"
      ],
      "outputs": [
        "packages/*/dist/**"
      ]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "storybook": {
      "cache": false,
      "persistent": true
    },
    "//#stencil-react#install": {
      "dependsOn": [
        "//#stencil-library#install",
        "//#stencil-library#build"
      ]
    }
  }
}