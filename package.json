{
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "react:dev": "turbo dev --filter=react-client",
    "storybook": "turbo storybook",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean:node_modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "clean:dist": "find . -name 'dist' -type d -prune -exec rm -rf '{}' +",
    "clean": "pnpm clean:node_modules && pnpm clean:dist",
    "install:all": "turbo app-install",
    "install:full": "pnpm install && turbo packages-build --filter=./packages/* && turbo app-install --filter=./apps/*",
    "install:apps": "turbo app-install --filter=./apps/*",
    "install:packages": "turbo packages-build --filter=./packages/*"
  },
  "devDependencies": {
    "eslint": "^9.14.0",
    "prettier": "^3.3.3",
    "turbo": "^2.2.3"
  },
  "packageManager": "pnpm@9.13.0",
  "name": "with-vite"
}